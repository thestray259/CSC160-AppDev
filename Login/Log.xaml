<Window x:Class="Login.Log"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Login"
        mc:Ignorable="d"
        Title="Log" Height="200" Width="500">
    <WrapPanel Margin="10">
        <TextBlock Text="Username:" FontSize="16" Margin="10" Width="80" Height="30"/>
        <TextBox x:Name="tbUserNameLogin" Width="350" Height="30"/>

        <TextBlock Text="Password:" FontSize="16" Margin="10" Width="80" Height="30"/>
        <TextBox x:Name="tbPasswordLogin" Width="350" Height="30"/>

        <Button x:Name="btnLogin" Content="Login" Margin="10" Height="30" FontSize="16" Width="220" Click="btnLogin_Click"/>
        <Button x:Name="btnGoBack" Margin="10" Content="Go Back" FontSize="16"  Width="220" Click="btnGoBack_Click"/>
    </WrapPanel>
</Window>
